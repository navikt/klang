{
  "name": "klang-server",
  "version": "1.0.0",
  "private": true,
  "author": "NAV",
  "license": "MIT",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "bun run build --watch & NAIS_CLUSTER_NAME=local node --watch --trace-warnings dist/server.js",
