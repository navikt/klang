    "@fastify/http-proxy": "11.4.1",
    "@fastify/type-provider-typebox": "6.1.0",
    "@navikt/oasis": "4.1.1",
    "fastify": "5.7.2",
    "fastify-metrics": "12.1.0",
    "happy-dom": "20.4.0",
    "prom-client": "15.1.3"
  },
  "devDependencies": {
    "@types/bun": "1.3.8",
    "@types/node": "25.2.0",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.3"
  }
}
