    "@fastify/http-proxy": "11.3.0",
    "@fastify/type-provider-typebox": "6.0.0",
    "@navikt/oasis": "3.8.0",
    "fastify": "5.6.1",
    "fastify-metrics": "12.1.0",
    "happy-dom": "18.0.1",
    "prom-client": "15.1.3"
  },
  "devDependencies": {
    "@types/bun": "1.2.22",
    "@types/node": "24.5.2",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.2"
  }
}
