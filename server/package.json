{
  "name": "klang-server",
  "version": "1.0.0",
  "private": true,
  "author": "NAV",
  "license": "MIT",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "bun run build --watch & NAIS_CLUSTER_NAME=local node --watch --trace-warnings dist/server.js",
    "build": "bun build ./src/server.ts --target node --format esm --sourcemap --outdir dist",
    "typecheck": "tsc --noEmit",
    "lint": "biome check"
  },
  "dependencies": {
    "@fastify/cors": "10.0.1",
    "@fastify/http-proxy": "10.0.1",
    "@fastify/type-provider-typebox": "5.0.1",
    "fastify": "5.1.0",
    "fastify-metrics": "12.1.0",
    "happy-dom": "15.11.0",
    "jose": "5.8.0",
    "openid-client": "5.6.5",
    "prom-client": "15.1.3",
    "redis": "4.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "1.1.13",
    "@types/node": "22.9.0",
    "typescript": "5.6.3"
  }
}
